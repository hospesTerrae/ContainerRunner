{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.ffff"
      }
    }
  },
  "CreationBackgroundService": {
    "ParallelismDegree": 5
  },
  "DestroyingBackgroundService": {
    "ParallelismDegree": 5
  },
  "DockerApiService": {
    "SocketAddr": "unix:///var/run/docker.sock"
  },
  "AllowedHosts": "*"
}
